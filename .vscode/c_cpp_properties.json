{
  "configurations": [
    {
      "name": "avr",
      "includePath": [
        "${workspaceFolder}/**",
        "/usr/lib/avr/includes/**"
      ],
      "defines": [
        "__AVR_ATmega328P__",
        "__AVR__",
        "__AVR_ENHANCED__"
      ],
      "compilerPath": "/usr/bin/avr-gcc",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "gcc-x64",
      "browse": {
        "path": ["${workspaceFolder}", "/usr/lib/avr"],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      }
    }
  ],
  "version": 4
}
